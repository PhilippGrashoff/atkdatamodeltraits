{
  "name": "philippgrashoff/atkdatamodeltraits",
  "description": "A small collection of traits to be used with Atk4\\Data\\Model",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "8.*",
    "atk4/data": "4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.25",
    "phpstan/phpstan": "1.*"
  },
  "autoload": {
    "psr-4": {
      "PhilippR\\Atk4\\ModelTraits\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhilippR\\Atk4\\ModelTraits\\Tests\\": "tests"
    }
  }
}
