{
  "name": "philippgrashoff/atkdatamodeltraits",
  "description": "A small collection of traits to be used with Atk4\\Data\\Model",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "8.2"
    }
  },
  "require": {
    "atk4/data": "4.*",
    "philippgrashoff/atkextendedtestcase": "4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.5",
    "phpstan/phpstan": "1.*"
  },
  "autoload": {
    "psr-4": {
      "atkdatamodeltraits\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "atkdatamodeltraits\\tests\\": "tests"
    }
  }
}
